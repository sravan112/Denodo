# REQUIRES-PROPERTIES-FILE - # Do not remove this comment!
CREATE OR REPLACE DATASOURCE JDBC ds_redshift_test
    DRIVERCLASSNAME = 'com.amazon.redshift.jdbc41.Driver'
    DATABASEURI = '${databases.tttt.datasources.jdbc.ds_redshift_test.DATABASEURI}'
    USERNAME = '${databases.tttt.datasources.jdbc.ds_redshift_test.USERNAME}'
    USERPASSWORD = '${databases.tttt.datasources.jdbc.ds_redshift_test.USERPASSWORD}' ${databases.tttt.datasources.jdbc.ds_redshift_test.USERPASSWORD.ENCRYPTED}
    CLASSPATH = 'amazon-redshift'
    DATABASENAME = 'redshift'
    DATABASEVERSION = '2015'
    FETCHSIZE = 1000
    VALIDATIONQUERY = 'Select 1'
    INITIALSIZE = 4
    MAXIDLE = -1
    MINIDLE = 0
    MAXACTIVE = 20
    EXHAUSTEDACTION = 1
    TESTONBORROW = true
    TESTONRETURN = false
    TESTWHILEIDLE = false
    TIMEBETWEENEVICTION = -1
    NUMTESTPEREVICTION = 3
    MINEVICTABLETIME = 1800000
    POOLPREPAREDSTATEMENTS = false
    MAXSLEEPINGPS = 4
    INITIALCAPACITYPS = 8
    DATA_LOAD_CONFIGURATION (
        BATCHINSERTSIZE = 50000
    )
    PROCESSING_UNITS = 2
    CPUS_PER_PROCESSING_UNIT = 16;

